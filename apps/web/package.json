{
  "name": "@resolvex/web",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite build && wrangler pages dev --port 3000 ./dist",
    "type:check": "tsc --noEmit",
    "deploy:preview": "vite build --mode=preview && wrangler pages deploy --project-name=resolvex-web-preview ./dist",
    "deploy:production": "vite build --mode=production && wrangler pages deploy --project-name=resolvex-web-production ./dist"
  },
  "dependencies": {
    "@resolvex/api": "workspace:^",
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-router": "0.0.1-beta.185",
    "@trpc/client": "^10.38.1",
    "@trpc/react-query": "^10.38.1",
    "million": "2.6.0-beta.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "valibot": "^0.12.0"
  },
  "devDependencies": {
    "@resolvex/tsconfig": "workspace:^",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "vite": "^4.4.9"
  }
}
