{
  "name": "@resolvex/monorepo",
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint && eslint --max-warnings 0 --report-unused-disable-directives .",
    "lint:fix": "turbo run lint:fix && eslint --fix --max-warnings 0 --report-unused-disable-directives .",
    "type:check": "turbo run type:check",
    "preview": "turbo run preview",
    "deploy:preview": "turbo run deploy:preview",
    "deploy:production": "turbo run deploy:production"
  },
  "devDependencies": {
    "@antfu/eslint-config-react": "^0.41.0",
    "eslint": "^8.49.0",
    "turbo": "latest",
    "typescript": "^5.2.2",
    "wrangler": "^3.7.0"
  }
}
